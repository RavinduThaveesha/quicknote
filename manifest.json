{
 "manifest_version": 2,
 "name": "Qukick Note",
 "description": "Chrome extension to take notes while browse",
 "version": "1",
 "author": "Ravindu Thaveesha",
 "background": {
   "scripts":  ["background.js"]
 },
 "browser_action": {
   "default_title": "Qukick Note",
   "default_popup": "popup.html",
   "default_icon": "/images/papers-64.png"
 },
 "permissions": [
	"activeTab",
    "contextMenus",
    "storage",
    "tabs",
    "webNavigation"
  ],
  "icons": {
    "16": "/images/papers-16.png",
    "48": "/images/papers-64.png",
    "128":"/images/papers-64.png"
   }
}